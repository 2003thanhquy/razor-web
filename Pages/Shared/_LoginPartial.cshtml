@using  Microsoft.AspNetCore.Identity
@using App.Models

@inject SignInManager<AppUser> SignInManager
@inject UserManager<AppUser> UserManager



<ul class="navbar-nav">
    @if(SignInManager.IsSignedIn(User) ){
         <li class="nav-item">
        <a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Manage/Index">
        @UserManager.GetUserName(User);
        </a>
    </li>
    <li class="nav-item">
       <form class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Page("/", new { area = "" })" method="post">
                <button type="submit" class="nav-link text-dark btn btn-link ">Dang xuat</button>
        </form>
    </li>
    }else{
         <li class="nav-item">
        <a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Register">Day ky</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Login">Dang nhap</a>
    </li>
    }
   

</ul>